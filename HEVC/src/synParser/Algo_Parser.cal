	uint(size= 8) num_ref_idx_l0_active_minus1;
	uint(size= 8) num_ref_idx_l1_active_minus1;
	uint(size= 8) num_ref_idx_lc_active_minus1;
	
	read_SliceHeader.se_idx_2.read : action ==>
	guard
		se_idx = 2 and isFifoFull(fifo)
	var
